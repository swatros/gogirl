
<div id="map" class = "h-100"
  data-navigation-ui-target="map"
  data-map-target="map"
  data-origin = "<%= @origin.to_json %>"
  data-incidents = "<%= @incidents.to_json %>"
  data-avoid = "<%= @incidents_avoid %>"
  data-destination = "<%= @destination.to_json %>"
  data-here-api-key="<%= ENV['HERE_API_KEY'] %>">
  <div class = "navigation-summary-div"
    data-map-target="summary">
    <%= render 'shared/navigation_summary' %>
  </div>
</div>
<div class = "navigation-directions-toggle-div"
  data-action="click->navigation-ui#switch">
  <%= render 'shared/directions_toggle' %>
</div>
